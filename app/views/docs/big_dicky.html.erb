<div class="d-flex align-items-center justify-content-center bg-primary pt-5 pb-3">
  
</div>
<div class="d-flex align-items-start justify-content-start">
  <div class="d-flex flex-column flex-shrink border-end">
    <% Doc.all.map do |doc| %>
      <div onclick="get_doc('<%= doc_path(doc.id) %>')" class="nav-link pointer">
        <%= doc.topic.classify %>
      </div>
    <% end %>
  </div>
  <div class="d-flex flex-column flex-fill" id="docs">
    <div class="text-center col-12">Docs</div>
  </div>
</div>
