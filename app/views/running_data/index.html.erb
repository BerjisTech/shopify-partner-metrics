<p id="notice"><%= notice %></p>

<h1>Running Data</h1>

<table>
  <thead>
    <tr>
      <th>Date</th>
      <th>Gross paying mrr</th>
      <th>Gross trial mrr</th>
      <th>Gross paying users</th>
      <th>Gross trial users</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @running_data.each do |running_datum| %>
      <tr>
        <td><%= running_datum.date %></td>
        <td><%= running_datum.gross_paying_mrr %></td>
        <td><%= running_datum.gross_trial_mrr %></td>
        <td><%= running_datum.gross_paying_users %></td>
        <td><%= running_datum.gross_trial_users %></td>
        <td><%= link_to 'Show', running_datum %></td>
        <td><%= link_to 'Edit', edit_running_datum_path(running_datum) %></td>
        <td><%= link_to 'Destroy', running_datum, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Running Datum', new_running_datum_path %>
