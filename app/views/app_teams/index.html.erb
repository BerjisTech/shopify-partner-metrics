<p id="notice"><%= notice %></p>

<h1>App Teams</h1>

<table>
  <thead>
    <tr>
      <th>Business</th>
      <th>App</th>
      <th>User</th>
      <th>Added by</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @app_teams.each do |app_team| %>
      <tr>
        <td><%= app_team.business_id %></td>
        <td><%= app_team.app_id %></td>
        <td><%= app_team.user_id %></td>
        <td><%= app_team.added_by %></td>
        <td><%= link_to 'Show', app_team %></td>
        <td><%= link_to 'Edit', edit_app_team_path(app_team) %></td>
        <td><%= link_to 'Destroy', app_team, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New App Team', new_app_team_path %>
