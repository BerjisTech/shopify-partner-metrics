<p id="notice"><%= notice %></p>
<h1>Billings</h1>
<table>
  <thead>
    <tr>
      <th>App</th>
      <th>User</th>
      <th>Business</th>
      <th>Amount</th>
      <th>Plan</th>
      <th colspan="3"></th>
    </tr>
  </thead>
  <tbody>
    <% @billings.each do |bill| %>
      <tr>
        <td><%= bill.app_id %></td>
        <td><%= bill.user_id %></td>
        <td><%= bill.business_id %></td>
        <td><%= bill.amount %></td>
        <td><%= bill.plan_id %></td>
        <td><%= link_to 'Show', bill_path(bill.id) %></td>
      </tr>
    <% end %>
  </tbody>
</table>
<br>
